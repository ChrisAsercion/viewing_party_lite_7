<h1><%= @user.name %>'s Dashboard</h1>
<h1>Welcome, <%= @user.name %></h1>
<p>Email: <%= @user.email %></p>



<section>
  <ul>
    <li>
      <p> <%= button_to "Discover Movies", user_discover_index_path(@user), method: :get  %></p>
    </li>
  </ul>
</section>

<div id="viewing">
<h2>Viewing Parties</h2>
    <ul>
      <% @user.viewing_parties.each do |p| %>
        <li>
          <p><%= p.movie_title %></p>
        </li>
      <% end %>
    </ul>
</div>